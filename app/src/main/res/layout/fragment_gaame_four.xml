        app:layout_constraintTop_toTopOf="@id/thththt" />


    <ProgressBar
        android:id="@+id/prog_bbbb"
        style="?android:attr/progressBarStyleHorizontal"
        android:layout_width="0dp"
        android:layout_height="10dp"
        android:layout_marginTop="12dp"
        android:progressDrawable="@drawable/prog"
        app:layout_constraintEnd_toEndOf="@id/ftgyhy"
        app:layout_constraintStart_toStartOf="@id/frgthy"
        app:layout_constraintTop_toBottomOf="@id/img_main_enemeefrgy"
        tools:progress="45" />



</androidx.constraintlayout.widget.ConstraintLayout>