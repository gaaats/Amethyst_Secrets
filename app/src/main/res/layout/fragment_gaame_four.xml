        android:background="#9255F4"
        android:fontFamily="@font/montserrat_black"
        android:padding="2dp"
        android:text=": Destroy this emeny"
        android:textAllCaps="true"
        android:textColor="@color/black"
        android:textSize="17sp"
        app:layout_constraintBottom_toBottomOf="@id/cbvdvgfrvfrr"
        app:layout_constraintEnd_toEndOf="@id/ftgyhy"
        app:layout_constraintStart_toEndOf="@id/cbvdvgfrvfrr"
        app:layout_constraintTop_toTopOf="@id/cbvdvgfrvfrr" />


    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/jhihyoihyiohyji"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.95" />


    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/rfrgtgyhy"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.40" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/thththt"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.30" />


    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/rfrgtjytohjoy"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.05" />



    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/frgthy"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.05" />



    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/ftgyhy"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
